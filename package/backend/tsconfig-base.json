{
  // receive control from tsconfig-output
  // Pass control over to root tsconfig-setting/
  "extends": "../../tsconfig-setting.json",

  // pass Set options for import and rootDir to root tsconfig-setting
  "complerOptions": {
    // "importHelpers": true,
    "rootDir": "./"
  },

  // pass references to root tsconfig-setting
  "references": [
    // reference points
  ],

  // pass include to root tsconfig-setting
  "include": ["./src/**/*.ts"],

  // exclude
  "exclude": ["node_modules"]
}
