{
  "name": "ethereum",
  "version": "1.0.0",
  "main": "./output/server",
  "scripts": {
    "dev": "concurrently --kill-others-on-fail --restart-tries 0 -n LINT,CODE -c grey,blue \"npm run format\" \"nodemon -w\"",
    "ts-lint": "read -p \"`ts-standard --verbose --fix`\"",
    "format": "prettier ../../../ --write",
    "type": "tsc -w",
    "start": "concurrently --kill-others-on-fail --restart-tries 0 -n COMPILER,CODE -c grey,blue \"npm run type\" \"npm run dev\""
  },
  "devDependencies": {
    "@openzeppelin/contracts": "^4.3.3",
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/request": "^2.48.7",
    "chai": "^4.3.4",
    "ethers": "^5.5.1",
    "hardhat": "^0.0.7",
    "solc": "^0.8.10",
    "ts-node": "^10.4.0"
  },
  "dependencies": {
    "crypto-js": "^4.1.1",
    "mkdirp": "^1.0.4",
    "multiformats": "^9.4.10",
    "request": "^2.88.2",
    "uuid": "^7.0.3"
  }
}
