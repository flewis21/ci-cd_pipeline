{
  "extends": "./backend/tsconfig.json",
  "compilerOptions": {
    "outDir": "./backend/output/",
    "strict": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "importHelpers": true,
    "forceConsistentCasingInFileNames": true,
    "skipDefaultLibCheck": true,
    "checkJs": true,
    "allowJs": true
  },
  "files": [],
  "exclude": ["node_modules", "./backend/output/"]
}
