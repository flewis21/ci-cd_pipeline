{
	"compilerOptions": {
		"outDir": "frontend/macros_and_custom_functions/output",
		"experimentalDecorators": true,
		"isolatedModules": false,
		"noImplicitUseStrict": true,
		"module": "None",
		"noLib": false,
		"noResolve": true,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"target": "ESNext",
		"strictNullChecks": true,
	},
	"include": ["frontend/macros_and_custom_functions/src/customFunctions.ts"],
	"exclude": ["node_modules"]
}
